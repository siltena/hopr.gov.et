  <script language="javascript">
            function postRequest(strURL){
                var xmlHttp;
                if(window.XMLHttpRequest){ // For Mozilla, Safari, ...
                    var xmlHttp = new XMLHttpRequest();
                }
        else if(window.ActiveXObject){ // For Internet Explorer
            var xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
        }
        xmlHttp.open('post', strURL, true);



        xmlHttp.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');



        xmlHttp.onreadystatechange = function(){
            if (xmlHttp.readyState == 4){

               document.getElementById("time").innerHTML=xmlHttp.responseText;


            }
        }
       xmlHttp.send(null);
    }

function call_login(){



    var searchvalue=document.getElementById("yy").value;
     var cat=document.getElementById("cat").value;
    var la=document.getElementById("lan").value;
   var searchby="date"
          

  var url = "search.jsp?searchvalue=" + searchvalue+"&searchby="+searchby+"&category="+cat+"&language="+la;

    postRequest(url);
}

function call_login1(){




    var searchvalue=document.getElementById("ty").value;
     var cat=document.getElementById("cat").value;
    
   var searchby="title"
   

  var url = "search.jsp?searchvalue=" + searchvalue+"&searchby="+searchby+"&category="+cat;

    postRequest(url);
}
function call_login2(){

  var searchvalue=document.getElementById("billno").value;
     var cat=document.getElementById("cat").value;

   var searchby="billno"


  var url = "search.jsp?searchvalue=" + searchvalue+"&searchby="+searchby+"&category="+cat;

    postRequest(url);
}



